    .port_info 3 /INPUT 1 "clk";
    .port_info 4 /INPUT 1 "clr";
v0x561192a70f90_0 .net "clk", 0 0, v0x561192abf710_0;  1 drivers
v0x561192abf2d0_0 .net "clr", 0 0, v0x561192abf7d0_0;  1 drivers
v0x561192abf390_0 .net "data_in", 7 0, v0x561192abf8a0_0;  1 drivers
v0x561192abf480_0 .var "data_out", 7 0;
v0x561192abf560_0 .net "load", 0 0, v0x561192abfa70_0;  1 drivers
E_0x561192aac320 .event posedge, v0x561192a70f90_0, v0x561192abf2d0_0;
    .scope S_0x561192a70d90;
T_0 ;
    %wait E_0x561192aac320;
    %load/vec4 v0x561192abf2d0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x561192abf480_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561192abf560_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x561192abf390_0;
    %assign/vec4 v0x561192abf480_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x561192abf480_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x561192abf480_0;
    %parti/s 1, 7, 4;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x561192abf480_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561192a724a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561192abf710_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x561192a724a0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x561192abf710_0;
    %inv;
    %store/vec4 v0x561192abf710_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x561192a724a0;
T_3 ;
